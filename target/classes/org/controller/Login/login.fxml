                    <TextField fx:id="txtfUsername" editable="true" promptText="Username" maxWidth="400.0" />
                </HBox>
                <Label text="Senha" />
                <HBox alignment="CENTER">
                    <PasswordField fx:id="txtfPassword" editable="true" promptText="Password" maxWidth="400.0" />
